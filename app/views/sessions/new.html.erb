<h1>Login</h1>


<%= flash[:error] %>



<%= form_for :session, url: sessions_path  do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In"%>
<% end %>

<br>
Not a Member?   <%= link_to "Sign Up", new_user_path %> 