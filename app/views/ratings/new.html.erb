<h4><%= Rating %></h4>

<%if flash[:errors] %>
    <% flash[:errors].each do |error| %><br>
    <%= error %><br>
    <% end %>
<% end %>

<%= form_for @rating do |f| %><br>
<br>
<%= f.collection_select :movie_id, Movie.all, :id, :title %><br>
<br>
<%= f.label :rate  %><br>
<%= f.number_field :rate %><br>

<%= f.submit %><br>
<% end %>
